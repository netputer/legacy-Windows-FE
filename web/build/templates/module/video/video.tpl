<templates>
    <script type="text/template" id="video_thread">
        <dl class="w-video-thread">          
            <dd class="w-video-thread-items cf"></dd>
            <dt class="w-video-thread-title"><input type="checkbox" class="w-video-thread-checkbox"></input><b></b></dt>        
        </dl>
    </script>
    <script type="text/template" id="video_item">
        <div class="w-video-item-wrapper">
            <img src="images/video/default.png">
            <div class="w-video-item-mask"></div>
            <input type="checkbox" class="w-video-item-checkbox item-hide"></input>
        </div>
    </script>
    <script type="text/template" id="video_info">
        <div class="w-video-item-info">
            <p class="w-video-name"></p>
            <p><span class="w-video-duration"></span><span class="w-video-size"></span></p>
        </div>
    </script>
</templates>
